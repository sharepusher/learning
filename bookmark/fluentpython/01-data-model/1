from math import hypot

class Vector:
    """Vector implementing through the use of the special methods __repr__, __abs__, __add__
    and __mul__. 
    """

    def __init__(self, x=0, y=0):
        self.x = x
        self.y = y

    def __repr__(self):
        return 'Vecotr(%r, %r)' % (self.x, self.y)    
     
